  "version": "0.0.1",
  "homepage": "https://github.com/jasancheg/sails-angular-electron",
  "authors": [
    "Jose Antonio Sanchez"
  ],
  "description": "Basic cross platform desktop application built with SailsJS, AngularJS and Electron",
