{
  "name": "sails-angular-electron",
  "appName": "BasicApp",
  "version": "0.0.1",
  "private": true,
  "description": "Basic cross platform desktop application built with SailsJS, AngularJS and Electron",
  "main": "./app/app.js",
  "license": "MIT",
  "author": {
    "name": "Jose Antonio Sanchez",
    "email": "jasancheg@email.com",
    "url": "inideaweb.com"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "electron-version": "0.30.0",
  "clientAppPath": "app/client",
  "serverAppPath": "app/server",
  "scripts": {
    "start": "grunt start",
    "startd": "grunt start:devtools",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "electron-packager ./dist BasicApp --out=releases --prune --asar --version=0.30.0 --icon=./app/icons/smile_128x128.ico --overwrite --all",
    "build-mac": "electron-packager ./dist BasicApp --out=releases  --prune --asar --version=0.30.0 --icon=./dist/app/icons/smile_128x128.icns --overwrite --platform=darwin --arch=x64",
    "build-win32": "electron-packager ./dist BasicApp --out=releases --prune --asar --version=0.30.0 --icon=./app/icons/smile_128x128.ico --overwrite --platform=win32 --arch=ia32 ",
    "build-win64": "electron-packager ./dist BasicApp --out=releases --prune --asar --version=0.30.0 --icon=./app/icons/smile_128x128.ico --overwrite --platform=win32 --arch=x64",
    "build-linux32": "electron-packager ./dist BasicApp --out=releases --prune --asar --version=0.30.0 --icon=./app/icons/smile_128x128.ico --overwrite --platform=linux --arch=ia32",
    "build-linux64": "electron-packager ./dist BasicApp --out=releases --prune --asar --version=0.30.0 --icon=./app/icons/smile_128x128.ico --overwrite --platform=linux --arch=x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasancheg/sails-angular-electron.git"
  },
  "keywords": [
    "SailsJS",
    "AngularJS",
    "ELectron",
    "Rest API",
    "UI"
  ],
  "bugs": {
    "url": "https://github.com/jasancheg/sails-angular-electron/issues"
  },
  "homepage": "https://github.com/jasancheg/sails-angular-electron#readme",
  "devDependencies": {
    "electron-packager": "^5.0.1",
    "electron-prebuilt": "^0.30.4",
    "grunt": "^0.4.5",
    "grunt-angular-templates": "^0.5.7",
    "grunt-autoprefixer": "^2.0.0",
    "grunt-bg-shell": "^2.3.1",
    "grunt-concurrent": "^1.0.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-compass": "^1.0.0",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-connect": "^0.9.0",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-contrib-cssmin": "^0.12.0",
    "grunt-contrib-htmlmin": "^0.4.0",
    "grunt-contrib-imagemin": "^0.9.2",
    "grunt-contrib-jshint": "^0.11.0",
    "grunt-contrib-uglify": "^0.7.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-filerev": "^2.1.2",
    "grunt-newer": "^1.1.0",
    "grunt-ng-annotate": "^0.9.2",
    "grunt-shell": "^1.1.2",
    "grunt-svgmin": "^2.0.0",
    "grunt-usemin": "^3.0.0",
    "grunt-wiredep": "^2.0.0",
    "jit-grunt": "^0.9.1",
    "jshint-stylish": "^1.0.0",
    "time-grunt": "^1.0.0"
  },
  "dependencies": {
    "electron-debug": "^0.1.1",
    "jquery": "^2.1.4",
    "sails.io.js": "^0.11.6",
    "socket.io-client": "^1.3.6"
  }
}
